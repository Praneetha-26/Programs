    "#Finding the largest number among n numbers\n",
    "n=int(input(\"enter n value:\"))\n",
    "num=int(input(\"Enter number 1:\"))\n",
    "largest=num\n",
    "for i in range(2,n+1):\n",
    "    num=int(input(f\"Enter number {i}:\"))\n",
    "    if num>largest:\n",
    "        largest=num\n",
    "print(\"The largest num is:\",largest)"
