   "source": [
    "def single_digit(a):\n",
    "    while a>=10:\n",
    "        sum=0\n",
    "        while a>0:\n",
    "            sum+=a%10\n",
    "            a//=10\n",
    "        a=sum\n",
    "    return a\n",
    "a=int(input(\"Enter number:\"))\n",
    "value=single_digit(a)\n",
    "print(value)"
   ]
