   "source": [
    "#Sum of n numbers using for loop\n",
    "n=int(input(\"enter the number:\"))\n",
    "sum=0\n",
    "for i in range(n+1):\n",
    "    sum=sum+i\n",
    "print(\"sum of n numbers:\",sum)"
   ]
