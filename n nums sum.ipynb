    "#using while \n",
    "n=int(input(\"enter the value:\"))\n",
    "i=1\n",
    "sum=0\n",
    "while i<=n:\n",
    "    sum=sum+i\n",
    "    i=i+1\n",
    "    print(\"sum of n numbers:\",sum)"
