   "source": [
    "def magic_num(n):\n",
    "    sum=n\n",
    "    while sum>9:\n",
    "        temp=sum\n",
    "        sum=0\n",
    "        while temp>0:\n",
    "            sum=sum+temp%10\n",
    "            temp//=10\n",
    "    if sum==1:\n",
    "        print(\"Magic Number\")\n",
    "    else:\n",
    "        print(\"not a magic number\")\n",
    "magic_num(1729)"
   ]
